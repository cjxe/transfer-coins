{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:@next/next/recommended",
        "prettier",
        "plugin:storybook/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "react/react-in-jsx-scope": 0,
        "arrow-body-style": [
            "error",
            "always"
        ],
        "react/prop-types": 0,
        "react/destructuring-assignment": 0,
        "react/jsx-props-no-spreading": 0
    }
}
